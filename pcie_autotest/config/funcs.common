    done
}
export -f DicLeftValue

SetIPByPairNicName()
{
    local varDicNicNm=${1}
    local varDicIndex=${2}
    local varDicPair=${3}
    local varDicIPs=${4}

    local bClue nA
    bClue=false
    if [ -n "${varDicIndex}" ]; then
        declare -p ${varDicIndex} 2>/dev/null |grep -q "declare -A ${varDicIndex}"
        if [ $? -eq 0 ]; then
            eval nA=\${#${varDicIndex}[@]}
            if [ ${nA} -gt 0 ]; then
                bClue=true
            fi
        fi
    fi

    local nMaxA sKeysA n1 key1 v1
    eval nMaxA=\${#${varDicIPs}[@]}
    eval sKeysA=\${!${varDicNicNm}[*]}
    n1=0
    if ${bClue}; then
        for key1 in ${sKeysA}; do
            eval ${varDicNicNm}[${key1}]=
        done

        while [ ${n1} -lt ${nMaxA} ]; do
            eval v1=\${${varDicIndex}[${n1}]}
            eval v1=\${${varDicPair}[${v1}]}
            eval ${varDicNicNm}[${v1}]=\${${varDicIPs}[${n1}]}
            let n1+=1
        done
    else
        for key1 in ${sKeysA}; do
            if [ ${n1} -lt ${nMaxA} ]; then
                eval ${varDicNicNm}[${key1}]=\${${varDicIPs}[${n1}]}
                eval ${varDicIndex}[${n1}]=${key1}
                let n1+=1
            else
                eval ${varDicNicNm}[${key1}]=
            fi
        done
    fi
}
export -f SetIPByPairNicName

