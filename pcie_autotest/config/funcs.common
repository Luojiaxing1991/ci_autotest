GetLinkIP()
{
    local ipPing=${1}
    local varIPLocal=${2}

    local ipsLink=
    local s1 s2
    if [ -z "${ipPing}" ]; then
        IFS=$'\n'; for s1 in ${g_IPPro}; do IFS=${g_IFS0};
            s2=$(sed "s/\(\.[0-9]\+\)\{1\}\$//;t;s/^.*\$//" <<< "${s1}")
            PingStatus "${s1}" "${s2}.1"
            if [ $? -eq 0 ]; then
                ipsLink=${ipsLink}${g__n}${s1}
            fi
        IFS=$'\n'; done; IFS=${g_IFS0};
    else
        IFS=$'\n'; for s1 in ${g_IPPro}; do IFS=${g_IFS0};
            PingStatus "${s1}" "${ipPing}"
            if [ $? -eq 0 ]; then
                ipsLink=${ipsLink}${g__n}${s1}
            fi
        IFS=$'\n'; done; IFS=${g_IFS0};
    fi

    local n1
    n1=$(sed '1d' <<< "${ipsLink}" |wc -l)
    if [ ${n1} -ne 1 ]; then
        g_sMsgCur="not get correct IP[${ipsLink}], maybe need set correct target IP for ping"
        g_sHeadCurLine=$(printf "%s[%3d]%s[%3d]" "${FUNCNAME[1]}" "${BASH_LINENO[0]}" "${FUNCNAME[0]}" ${LINENO})
        OutLogHead 1 "" "${g_sHeadCurLine}" "${g_sMsgCur}" "${flLog}" false
        return 1
    fi

    ipsLink=$(sed '1d' <<< "${ipsLink}")
    eval ${varIPLocal}=\${ipsLink}
}
export -f GetLinkIP

NicNamePair()
{
    local varDicBoard1=${1}
    local varDicBoard2=${2}
    local varDicNicNm1=${3}
    local varDicNicNm2=${4}
    local varDicNicPair=${5}
    local flLog=${6:-/dev/null}

    #设置探测IP;
    local sIPRemote1 sIPRemote2 sIPNetSect1 sIPNetSect2 n1 sIP1 sIP2
    eval sIPRemote1=\${${varDicBoard1}[kIP]}
    eval sIPRemote2=\${${varDicBoard2}[kIP]}
    if [ -z "${sIPRemote1}" ]; then
        GetLinkIP "${g_ipNet}" sIPRemote1
        if [ $? -ne 0 ]; then
            return 1
        fi
    fi

    sIPNetSect1=$(sed "s/\(\.[0-9]\+\)\{2\}\$//;t;s/^.*\$//" <<< "${sIPRemote1}")
    let n1=g_sIPBaseSect_1-1
    sIP1=${sIPNetSect1}.${g_sIPBaseSect_2}.${n1}

    sIPNetSect2=$(sed "s/\(\.[0-9]\+\)\{2\}\$//;t;s/^.*\$//" <<< "${sIPRemote2}")
    let n1=g_sIPBaseSect_1+1
    sIP2=${sIPNetSect2}.${g_sIPBaseSect_2}.${n1}

    local key1 key2 sIPA sIPB iR1 iR2 sLog1 dicNic1
    local sKeys1 sKeys2
    eval sKeys1=\${!${varDicNicNm1}[*]}
    eval sKeys2=\${!${varDicNicNm2}[*]}
    for key1 in ${sKeys1}; do
        for key2 in ${sKeys2}; do
            eval ${varDicNicNm1}[${key1}]=\${sIP1}
            eval ${varDicNicNm2}[${key2}]=\${sIP2}

            declare -A dicNic1=()
            DicLeftValue "${varDicNicNm1}" key1 dicNic1
            NicEthEnableRemote "${varDicBoard1}" dicNic1 "${flLog}"

            declare -A dicNic1=()
            DicLeftValue "${varDicNicNm2}" key2 dicNic1
            NicEthEnableRemote "${varDicBoard2}" dicNic1 "${flLog}"

            eval sIPB=\${${varDicNicNm2}[${key2}]}
            PingStatusRemote "${varDicBoard1}" "${key1}" "${sIPB}" "${flLog}"
            iR1=$?
            eval sIPA=\${${varDicNicNm1}[${key1}]}
            PingStatusRemote "${varDicBoard2}" "${key2}" "${sIPA}" "${flLog}"
            iR2=$?
            if [ ${iR1} -eq ${iR2} ]; then
                if [ ${iR1} -eq 0 ]; then
                    eval ${varDicNicPair}[${key1}]=\${key2}
                    break
                fi
            else
                g_sMsgCur="[ ${iR1} -ne ${iR2} ] mean is one side ping ok"
                g_sHeadCurLine=$(printf "%s[%3d]%s[%3d]" "${FUNCNAME[1]}" "${BASH_LINENO[0]}" "${FUNCNAME[0]}" ${LINENO})
                OutLogHead 1 "" "${g_sHeadCurLine}" "${g_sMsgCur}" "${flLog}" false
            fi
        done
    done

    local nPair
    eval nPair=\${#${varDicNicPair}[*]}
    if [ ${nPair} -lt 1 ]; then
        g_sMsgCur="match failed"
        if [ "${sIPRemote1}" == "${sIPRemote2}" ]; then
            if [ "${sKeys1}" == "${sKeys2}" ]; then
                g_sMsgCur=${g_sMsgCur}": may be the same machine?"
            fi
        fi
        g_sHeadCurLine=$(printf "%s[%3d]%s[%3d]" "${FUNCNAME[1]}" "${BASH_LINENO[0]}" "${FUNCNAME[0]}" ${LINENO})
        OutLogHead 1 "" "${g_sHeadCurLine}" "${g_sMsgCur}" "${flLog}" false
        return 1
    fi

    g_sMsgCur="match finished"
    g_sHeadCurLine=$(printf "%s[%3d]%s[%3d]" "${FUNCNAME[1]}" "${BASH_LINENO[0]}" "${FUNCNAME[0]}" ${LINENO})
    OutLogHead 0 "" "${g_sHeadCurLine}" "${g_sMsgCur}" "${flLog}" false
}
export -f NicNamePair

